####################################是否污染不变
set ylabel 'standard deviation'
set title 'every year every distance polluted standard deviation'
plot for [year in "2004 2005 2006"]  for [j = 1:2] for [i = 1:8] comData(i, year, j) @udm5 with linespoint pn 5 lt (j-1)*100+(abs(year)-2004)*8+i title year.clean[j]." ".distance[i]

################################年份不变
set title '2004 every distance polluted and nonpolluted standard deviation'
plot for [year in "2004"]  for [j = 1:2] for [i = 1:8] comData(i, year, j)  @udm5 with linespoint pn 5 lt (j-1)*100+(abs(year)-2004)*8+i title year.clean[j]." ".distance[i]

set title '2005 every distance polluted and nonpolluted standard deviation'
plot for [year in "2005"]  for [j = 1:2] for [i = 1:8] comData(i, year, j)  @udm5 with linespoint pn 5 lt (j-1)*100+(abs(year)-2004)*8+i title year.clean[j]." ".distance[i]

set title '2006 every distance polluted and nonpolluted standard deviation'
plot for [year in "2006"]  for [j = 1:2] for [i = 1:8] comData(i, year, j)  @udm5 with linespoint pn 5 lt (j-1)*100+(abs(year)-2004)*8+i title year.clean[j]." ".distance[i]

####################################x轴距离不变
do for [i = 1:8] {
   set title "every year ".distance[i]." polluted and nonpolluted standard deviation"
   plot for [j = 1:2] for [year in "2004 2005 2006"] comData(i, year, j) @udm5 with linespoint pn 5 lt (abs(year)-2003) title year.clean[j]." ".distance[i]
}


