#                                  #
####################################

set yrange [-3.5*10**(-4):0.5*10**(-4)]
#####################################是否污染不变
set ylabel 'atmospheric pressure (%)'
set title 'every year every distance polluted atmospheric pressure'
plot for [year in "2004 2005 2006"] for [i in "1 2 3 4 5 6 7 8"] comData(abs(i), year) @uds0 with linespoint pn 5 lt (abs(year)-2004)*8+abs(i) title year." ".distance[abs(i)]

####################################年份不变
set title '2004 every distance polluted and nonpolluted atmospheric pressure'
plot for [year in "2004"] for [i in "1 2 3 4 5 6 7 8"] comData(abs(i), year) @uds0 with linespoint pn 5 lt (abs(year)-2004)*8+abs(i) title year." ".distance[abs(i)]

