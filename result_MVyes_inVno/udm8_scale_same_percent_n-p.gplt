####################################x轴距离不变
do for [i in "1 2 3 4 5 6 7 8"] {
   set title "every year ".distance[abs(i)]." polluted and nonpolluted atmospheric pressure"
   plot for [year in "2004 2005 2006"] comData(abs(i), year) @uds0 with linespoint pn 5 lt (abs(year)-2003) title year." ".distance[abs(i)]
}


